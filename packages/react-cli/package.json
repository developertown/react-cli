{
  "name": "@developertown/react-cli",
  "description": "CLI for generating and maintaining react projects at DeveloperTown",
  "version": "0.6.2",
  "private": false,
  "author": "NullVoxPopuli",
  "bin": {
    "react": "./bin/run"
  },
  "bugs": "https://github.com/developertown/react-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.1",
    "cli-ux": "^5.2.1",
    "execa": "^1.0.0",
    "inquirer": "^6.2.2",
    "listr": "^0.14.3",
    "node-fetch": "^2.3.0",
    "shelljs": "^0.8.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/execa": "^0.9.0",
    "@types/inquirer": "^0.0.44",
    "@types/listr": "^0.13.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-fetch": "^2.1.7",
    "@types/shelljs": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "chai": "^4",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "prettier": "^1.16.4",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/developertown/react-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "react",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/developertown/react-cli",
  "scripts": {
    "lint:js": "yarn eslint . --ext .js --ext .ts --ext .jsx --ext .tsx",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
